@using ETicaretData.Identity
@model AppRole
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit Role</h1>
<hr />
<form method="post">
    <div class="form-group">
        <div class="col-md-3">
            @Html.LabelFor(m => m.Name)
        </div>
        <div class="col-md-9">
            @Html.EditorFor(m => m.Name, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Ok</button>
    </div>
</form>

